	<!-- FullCalendar -->
	<script>
	  $(window).load(function() {
		var date = new Date(),
			d = date.getDate(),
			m = date.getMonth(),
			y = date.getFullYear(),
			started,
			categoryClass;

		var calendar = $('#calendar').fullCalendar({
		  header: {
			left: 'prev,next today',
			center: 'title',
			right: 'month,agendaWeek,agendaDay'
		  },
		  selectable: false,
		  selectHelper: false,
		  editable: false,
		  /*
		  events: [{
				title: 'All Day Event',
				start: new Date(y, m, 1)
			  }, {
				title: 'Long Event',
				start: new Date(y, m, d - 5),
