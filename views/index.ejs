<body id="top">
  <div id="indexModal" class="modal">
    <div class="form">

      <ul class="tab-group">
        <li class="tab active"><a id="signup-tab-link" href="#signup">Sign Up</a></li>
        <li class="tab"><a id="login-tab-link" href="#login">Log In</a></li>
      </ul>

      <div class="tab-content">
        <div id="signup">
          <h1>Sign Up for Free</h1>

          <form action="/auth/signup" method="post" novalidate>

          <div class="top-row">
            <div class="item field-wrap">
              <label>
                First Name<span class="req">*</span>
              </label>
              <input type="text" name="first_name" required="required" autocomplete="off" />
            </div>

            <div class="item field-wrap">
              <label>
                Last Name<span class="req">*</span>
              </label>
              <input type="text" name="last_name" required="required" autocomplete="off"/>
            </div>
          </div>

          <div class="item field-wrap">
            <label>
              Email Address<span class="req">*</span>
            </label>
            <input type="email" type="email" name="email" required autocomplete="off"/>
          </div>

          <div class="item field-wrap">
            <label>
              Confirm Email Address<span class="req">*</span>
            </label>
            <input type="email" type="email" data-validate-linked='email' required autocomplete="off"/>
          </div>

          <div class="item field-wrap">
            <label>
              Set A Password<span class="req">*</span>
            </label>
            <input id="signupPassword" data-validate-length-range="8,25" type="password" name="password" required autocomplete="off"/>
          </div>

          <div class="item field-wrap">
            <label>
              Choose A Role<span class="req">*</span>
            </label>
            <select class="required select2_single" tabindex="-1" name="role" required>
              <option></option>
              <option value='mentee'>I Am A Mentee</option>
              <option value='mentor'>I Am A Mentor</option>
            </select>
          </div>

          <button type="submit" class="button button-block"/>Sign Up!</button>

          </form>

        </div>

        <div id="login">
          <h1>Welcome Back!</h1>

          <form action="/auth/login" method="post" novalidate>

            <div class="item field-wrap">
            <label>
              Email Address<span class="req">*</span>
            </label>
            <input type="email" name="email" required autocomplete="on"/>
          </div>

          <div class="item field-wrap">
            <label>
              Password<span class="req">*</span>
            </label>
            <input type="password" name="password" required autocomplete="on"/>
          </div>

          <p class="forgot"><a href="#">Forgot Password?</a></p>

          <button class="button button-block"/>Log In</button>

          </form>

        </div>

      </div><!-- tab-content -->

    </div> <!-- /form -->
  </div>

