<div class="right_col" role="main" style="min-height: none">
  <!-- complete profile -->
  <div class="complete-registration">

    <div class="row">

      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <h2>Complete Registration<small>You must complete the full registration to become a mentor</small></h2>
            <ul class="nav navbar-right panel_toolbox">
              <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
              </li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                <ul class="dropdown-menu" role="menu">
                  <li><a href="#">Settings 1</a>
                  </li>
                  <li><a href="#">Settings 2</a>
                  </li>
                </ul>
              </li>
              <li><a class="close-link"><i class="fa fa-close"></i></a>
              </li>
            </ul>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">


            <!-- Smart Wizard -->
            <p>This wizard will walk you through the registration process.</p>
            <div id="wizard" class="form_wizard wizard_horizontal">
              <ul class="wizard_steps">
                <li>
                  <a href="#step-1">
                    <span class="step_no">1</span>
                    <span class="step_descr">
                                      Step 1<br />
                                      <small>Personal Profile</small>
                                  </span>
                  </a>
                </li>
                <li>
                  <a href="#step-2">
                    <span class="step_no">2</span>
                    <span class="step_descr">
                                      Step 2<br />
                                      <small>Background Information</small>
                                  </span>
                  </a>
                </li>
                <li>
                  <a href="#step-3">
                    <span class="step_no">3</span>
                    <span class="step_descr">
                                      Step 3<br />
                                      <small>Additional Information</small>
                                  </span>
                  </a>
                </li>
                <li>
                  <a href="#step-4">
                    <span class="step_no">4</span>
                    <span class="step_descr">
                                      Step 4<br />
                                      <small>Terms of Service</small>
                                  </span>
                  </a>
                </li>
              </ul>
              <div id="step-1">
                <h2 class="StepTitle">Step 1 - Personal Profile</h2>
                <p>Just for our information</p>
                <form class="form-horizontal form-label-left" id="form-step-1">

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">First Name
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input type="text" class="form-control col-md-7 col-xs-12 has-feedback-left" id="first-name" readonly value=<%= user.profile.first_name %> >
                      <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Last Name
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input type="text" class="form-control col-md-7 col-xs-12 has-feedback-left" id="last-name" readonly value=<%= user.profile.last_name %> >
                      <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Email
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input id="email" type="email" name=email class="form-control col-md-7 col-xs-12 has-feedback-left" readonly value=<%= user.email %> >
                      <span class="fa fa-envelope form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Confirm Email <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input data-validate-linked='email' id="email2" type="email" class="form-control col-md-7 col-xs-12 has-feedback-left" required="required" placeholder="e.g. example@host.com">
                      <span class="fa fa-envelope form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item multi required form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Gender <span class="required">*</span></label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="required select2_single form-control" tabindex="-1" name="gender" style="width: 100%;">
                        <option></option>
                        <option value='male'>Male</option>
                        <option value='female'>Female</option>
                        <option value='other'>Other</option>
                        <option value='na'>Prefer Not To Disclose</option>
                      </select>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Age <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input id="age" class="form-control col-md-7 col-xs-12 has-feedback-left" required="required" type="number" data-validate-minmax="15, 28" name="age">
                      <span class="fa fa-birthday-cake form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Phone Number <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input id="phone" name="phone" data-validate-length-range="8,20" class="form-control col-md-7 col-xs-12 has-feedback-left" placeholder="e.g. (999) 999-9999" required="required" type="tel">
                      <span class="fa fa-phone form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>
                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Linkedin
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <input id="phone" name="linkedin" class="form-control col-md-7 col-xs-12 has-feedback-left" placeholder="e.g. www.example.com" type="text">
                      <span class="fa fa-linkedin form-control-feedback left" aria-hidden="true"></span>
                    </div>
                  </div>

                </form>

              </div>
              <div id="step-2">
                <h2 class="StepTitle">Step 2 - Background Information</h2>
                <p>This information is used to facilitate the mentor-match process</p>
                <form class="form-horizontal form-label-left" method="post" action="/mentor-complete" id="form-step-2">

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Current University/College <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="required select2_single form-control" tabindex="-1" name='curr_school' style="width: 100%;">
                        <option></option>
                        <option value="University of Waterloo">University of Waterloo</option>
                        <option value="University of Toronto">University of Toronto</option>
                        <option value="University of Western Ontario">University of Western Ontario</option>
                        <option value="University of British Columbia">University of British Columbia</option>
                        <option value="York University">York University</option>
                      </select>
                    </div>
                  </div>

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Major Field of Study <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="required select2_single form-control" tabindex="-1" name='curr_major' style="width: 100%;">
                        <option></option>
                        <option value="med">Medicine and Health Sciences</option>
                        <option value="cs">Computer Science</option>
                        <option value="eng">Engineering and Applied Sciences</option>
                        <option value="math">Mathematics and Physics</option>
                        <option value="fin">Accounting, Finance and Financial Mathematics</option>
                        <option value="econ">Business, Economics and Commerce</option>
                        <option value="arts">Arts and Humanities</option>
                        <option value="sci">Natural Sciences (Biology, Chemistry, Earth and Space)</option>
                        <option value="law">Political Science, Law and Societal Studies</option>
                        <option value="soci">Social Sciences (Psychology, Sociology, Nutrition Science)</option>
                      </select>
                    </div>
                  </div>

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Minor Field of Study (optional)
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="select2_single form-control" tabindex="-1" name='curr_minor' style="width: 100%;">
                        <option></option>
                        <option value="med">Medicine and Health Sciences</option>
                        <option value="cs">Computer Science</option>
                        <option value="eng">Engineering and Applied Sciences</option>
                        <option value="math">Mathematics and Physics</option>
                        <option value="fin">Accounting, Finance and Financial Mathematics</option>
                        <option value="econ">Business, Economics and Commerce</option>
                        <option value="arts">Arts and Humanities</option>
                        <option value="sci">Natural Sciences (Biology, Chemistry, Earth and Space)</option>
                        <option value="law">Political Science, Law and Societal Studies</option>
                        <option value="soci">Social Sciences (Psychology, Sociology, Nutrition Science)</option>
                      </select>
                    </div>
                  </div>

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">What Year Do You Graduate in? <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="required select2_single form-control" tabindex="-1" name='grad_year' style="width: 100%;">
                        <option></option>
                        <option value="2025">2025 or later</option>
                        <option value="2024">2024</option>
                        <option value="2023">2023</option>
                        <option value="2022">2022</option>
                        <option value="2021">2021</option>
                        <option value="2020">2020</option>
                        <option value="2019">2019</option>
                        <option value="2018">2018</option>
                        <option value="2017">2017</option>
                        <option value="2016">2016</option>
                        <option value="2015">2015 or earlier</option>
                      </select>
                    </div>
                  </div>

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Your Cumulative GPA <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <select class="required select2_single form-control" tabindex="-1" name='gpa' style="width: 100%;">
                        <option></option>
                        <option value="5">4.0+ (100% or more)</option>
                        <option value="4">4.0 (99% - 90%)</option>
                        <option value="3">3.9-3.0 (89%-75%)</option>
                        <option value="2">2.9 or below (74% or below)</option>
                      </select>
                    </div>
                  </div>



                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">You Were Once a Part of: <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12 has-feedback">
                      <select class="required select2_multiple_4 form-control" multiple="multiple" name="high_school_program" style="width: 100%;">
                        <option></option>
                        <option value="AP">AP (Advanced Placement)</option>
                        <option value="IB">IB (International Baccalaureate)</option>
                        <option value="TOPS">TOPS (Talented Offerings for Programs in the Sciences)</option>
                        <option value="ESL">ESL (English as a Second Language)</option>
                      </select>
                    </div>
                  </div>

                  <div class="item form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Your Skills And Extracurriculars <span class="required">*</span></label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <input id="tags_1" type="text" required="required" class="tags form-control" name="skills" />
                    </div>
                  </div>

                </form>
              </div>
              <div id="step-3">
                <h2 class="StepTitle">Step 3 Additional Information</h2>
                <p>So that we get to know you better!</p>
                <form class="form-horizontal form-label-left" id='form-step-3'>

                  <div class="item form-group">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                      <label class="control-label" for="textarea">What makes you a great mentor?</label>
                      <textarea id="additional-info1" data-validate-length-range="200,500" name="paragraphs" class="form-control col-md-7 col-xs-12" required="required"></textarea>
                    </div>
                  </div>
                  <div class="item form-group">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                      <label class="control-label" for="textarea">What do you hope to gain from being a mentor?</label>
                      <textarea id="additional-info2" data-validate-length-range="200,500" name="paragraphs" class="form-control col-md-7 col-xs-12" required="required"></textarea>
                    </div>
                  </div>
                  <div class="item form-group">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                      <label class="control-label" for="textarea">Tell us about a time you tried to teach someone about a subject.</label>
                      <textarea id="additional-info2" data-validate-length-range="200,500" name="paragraphs" class="form-control col-md-7 col-xs-12" required="required"></textarea>
                    </div>
                  </div>
                  <div class="item form-group">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                      <label class="control-label" for="textarea">Feel Free to Tell Us Anything Else. </label>
                      <textarea id="additional-info3" name="paragraphs" class="form-control col-md-7 col-xs-12"></textarea>
                    </div>
                  </div>
                </form>
                <div class="col-md-12 col-sm-12 col-xs-12">
                  <label class="control-label" for="textarea">Your Resume</label>
                  <form action="form_upload.html" class="dropzone"></form>
                </div>
              </div>
